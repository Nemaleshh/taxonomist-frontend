{
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python" 
      },
      {
        "src": "frontend/package.json", 
        "use": "@vercel/static-build" 
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "app.py"
      },
      {
        "src": "/(.*)", 
        "dest": "frontend/build/$1"
      }
    ]
  }